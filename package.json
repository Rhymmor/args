{
  "name": "args",
  "version": "1.0.1",
  "description": "Minimal toolkit for building CLIs",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "gulp",
    "prepublish": "gulp transpile",
    "pretest": "gulp transpile",
    "test": "ava"
  },
  "ava": {
    "files": [
      "test/*.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "repository": "leo/args",
  "keywords": [
    "cli",
    "command",
    "arguments",
    "util",
    "bin",
    "commander",
    "minimist",
    "binary",
    "parser",
    "option",
    "flags",
    "cmd",
    "utility",
    "helper",
    "terminal",
    "console"
  ],
  "author": "leo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leo/args/issues"
  },
  "homepage": "https://github.com/leo/args#readme",
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-plugin-add-module-exports": "^0.2.0",
    "babel-preset-es2015": "^6.6.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.0"
  },
  "dependencies": {
    "camelcase": "^3.0.0",
    "loud-rejection": "^1.3.0",
    "minimist": "^1.2.0",
    "pkginfo": "^0.4.0"
  }
}
